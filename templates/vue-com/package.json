    "lint": "eslint . --cache"
  },
  "devDependencies": {
    "@kecrily/eslint-config": "^0.1.12",
    "@vitejs/plugin-vue": "^3.0.3",
    "eslint": "^8.22.0",
    "typescript": "^4.7.4",
    "unplugin-auto-import": "^0.11.2",
    "unplugin-vue-components": "^0.22.4",
    "vite": "^3.0.8",
    "vite-plugin-dts": "^1.4.1",
    "vue": "^3.2.37",
    "vue-tsc": "^0.40.1"
  },
  "eslintConfig": {
    "extends": [
      "@kecrily"
    ]
