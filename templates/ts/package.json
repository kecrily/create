{
  "name": "ts-starter",
  "version": "0.0.0",
  "packageManager": "pnpm@7.1.5",
  "description": "",
  "keywords": [],
  "license": "MIT",
  "author": "Percy Ma",
  "repository": "https://github.com/kecrily/ts-starter",
  "homepage": "https://github.com/kecrily/ts-starter#readme",
  "bugs": "https://github.com/kecrily/ts-starter/issues",
  "funding": "https://github.com/sponsors/kecrily",
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "unbuild --stub",
    "build": "unbuild",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --cache",
    "try": "tsx src/index.ts"
  },
  "devDependencies": {
    "@kecrily/eslint-config": "^0.1.7",
    "@types/node": "^18.0.0",
    "eslint": "^8.18.0",
    "pnpm": "^7.3.0",
    "tsx": "^3.6.0",
    "typescript": "^4.7.4",
    "unbuild": "^0.7.4",
    "vite": "^2.9.12",
    "vitest": "^0.15.2"
  },
  "eslintConfig": {
    "extends": [
      "@kecrily"
    ]
  }
}
