{
  "name": "ts-starter",
  "version": "0.0.0",
  "description": "",
  "keywords": [],
  "license": "MIT",
  "author": "Percy Ma",
  "repository": "https://github.com/kecrily/ts-starter",
  "homepage": "https://github.com/kecrily/ts-starter#readme",
  "bugs": "https://github.com/kecrily/ts-starter/issues",
  "funding": "https://github.com/sponsors/kecrily",
  "packageManager": "pnpm@7.1.5",
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "unbuild --stub",
    "build": "unbuild",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "try": "jiti src/index.ts"
  },
  "devDependencies": {
    "@kecrily/eslint-config": "^0.1.4",
    "@types/node": "^17.0.35",
    "eslint": "^8.16.0",
    "jiti": "^1.13.0",
    "pnpm": "^7.1.5",
    "typescript": "^4.7.2",
    "unbuild": "^0.7.4",
    "vite": "^2.9.9",
    "vitest": "^0.12.9"
  }
}
