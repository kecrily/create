{
  "name": "ts-starter",
  "version": "0.0.0",
  "packageManager": "pnpm@7.0.0-rc.7",
  "description": "",
  "keywords": [],
  "homepage": "https://github.com/kecrily/ts-starter",
  "bugs": "https://github.com/kecrily/ts-starter/issues",
  "license": "MIT",
  "author": "Percy Ma",
  "repository": "github:kecrily/ts-starter",
  "funding": "https://github.com/sponsors/kecrily",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "try": "jiti src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@kecrily/eslint-config": "^0.1.1",
    "@types/node": "^17.0.25",
    "eslint": "^8.13.0",
    "jiti": "^1.13.0",
    "pnpm": "^7.0.0-rc.7",
    "typescript": "^4.6.3",
    "unbuild": "^0.7.4",
    "vite": "^2.9.5",
    "vitest": "^0.9.3"
  }
}
